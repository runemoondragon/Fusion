{\n  "on": [{\n    "type": "telegram.message.new"\n  }],\n  "jobs": [{\n    "id": "processMessage",\n    "type": "switch",\n    "cases": [{\n        "condition": "contains(trigger.message.text, 'invoice')",\n        "do": [{\n          "type": "gmail.send",\n          "to": "your_email@gmail.com",\n          "subject": "New Invoice Message",\n          "body": "{{trigger.message.text}}"\n        }]\n      }, {\n        "condition": "contains(trigger.message.text, 'calendar')",\n        "do": [{\n          "type": "google_calendar.create_event",\n          "event": {\n            "summary": "New Calendar Event",\n            "description": "{{trigger.message.text}}",\n            "start": {\n              "dateTime": "{{trigger.message.date}}",\n              "timeZone": "America/New_York"\n            },\n            "end": {\n              "dateTime": "{{trigger.message.date}}",\n              "timeZone": "America/New_York"\n            }\n          }\n        }]\n      }, {\n        "condition": "true",\n        "do": [{\n          "type": "telegram.message.send",\n          "chat_id": "{{trigger.message.chat_id}}",\n          "text": "Command not recognized."\n        }]\n      }]\n    }]\n  }]\n}